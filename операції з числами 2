using System;
namespace ConsoleApp9
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 1;
            a = a + 1;
            Console.WriteLine(a);
            a += 1;
             Console.WriteLine(a);
            a++;
            Console.WriteLine(a);
            ++a;
            Console.WriteLine(a);
            a--;
            Console.WriteLine(a);
            --a;
            Console.WriteLine(a);
            Console.ReadKey();
        }
    }
}
